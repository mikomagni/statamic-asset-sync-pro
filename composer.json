{
    "name": "mikomagni/statamic-rsync-tools",
    "version": "1.0.0",
    "license": "proprietary",
    "type": "statamic-addon",
    "minimum-stability": "stable",
    "description": "Statamic Rsync Tools - Powerful rsync commands for file synchronization between local and remote servers.",
    "keywords": ["statamic", "rsync", "sync", "deploy", "assets", "files", "remote", "transfer"],
    "homepage": "https://clandestina.com.au",
    "support": {
        "issues": "https://github.com/mikomagni/statamic-rsync-tools/issues",
        "source": "https://github.com/mikomagni/statamic-rsync-tools"
    },
    "authors": [
        {
            "name": "MikoMagni",
            "homepage": "https://clandestina.com.au"
        }
    ],
    "require": {
        "php": "^8.1",
        "statamic/cms": "^5.0",
        "laravel/prompts": "^0.1",
        "symfony/process": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "MikoMagni\\RsyncCommands\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Statamic Rsync Tools",
            "description": "Powerful rsync commands for file synchronization between local and remote servers",
            "developer": "MikoMagni",
            "developer_url": "https://clandestina.com.au"
        },
        "laravel": {
            "providers": [
                "MikoMagni\\RsyncCommands\\ServiceProvider"
            ]
        }
    }
}
